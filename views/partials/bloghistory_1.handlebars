<div class="bs-docs-section">
    <div class="row">
        <div class="col-lg-12">
            <div class="bs-component">

                {{#if bloghistory}}
                <div id="accordion" class="bloghistorybyuserid">
                    {{#Compare bloghistory.count ">" 0}} {{#each bloghistory.result}}

                    <div>
                        <h3>
                            <span>{{this.blogid}}</span>
                        </h3>
                        <div>test
                        </div>
                    </div>

                    {{/each}} {{/Compare}}
                </div>

                <input type="hidden" name="userid" id="userid" value="{{user._id}}" />
                <input type="hidden" name="username" id="username" value="{{user.username}}" />

                <div id="divImageBlogHistory" data-lastbloghistoryid="{{lastbloghistoryid}}">
                    <span id="loadmoreblogbyuserid" name="loadmoreblogbyuserid" alt="Load More" class="glyphicon glyphicon-hand-down"></span>
                </div>

                {{else}}
                <div class="nobloghistory hidden">
                    <strong>No records available!!!</strong>
                </div>

                {{/if}}
            </div>
        </div>
    </div>
</div>

