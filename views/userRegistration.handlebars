<div class="bs-docs-section">
    <div class="col-lg-12">
        <div class="col-lg-3">
        </div>
        <div class="col-lg-6 signUp">
            <div class="well bs-component">
                <dix class="form-horizontal">
                    <fieldset>
                        <legend>Sign-up</legend>
                        <div class="alert alert-dismissible alert-success successPanel hidden">
                        </div>
                        <div class="alert alert-dismissible alert-danger ErrorPanel hidden">
                        </div>
                        <div class="userRegistration">
                            <div class="form-group userName">
                                <label for="inputEmail" class="col-lg-2 control-label">Username</label>
                                <div class="col-lg-10 ">
                                    <input type="text" class="form-control input-sm" id="inputUserName" placeholder="Username">
                                    
                                    <div class="msg">
                                         {{!-- <span class="glyphicon glyphicon-ok form-control-feedback feedbackOK "></span> --}}
                                        {{!-- <span class="glyphicon glyphicon-ok form-control-feedback feedbackOK "></span> --}}
                                    {{!-- <span class="glyphicon glyphicon-exclamation-sign  form-control-feedback eedbackES "></span> --}}
                                    {{!-- <span class="help-block">please input email</span>
                                    <span class="help-block">wrong email</span>
                                    <span class="help-block">email already available</span> --}}

                                    </div>
                                                                      
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="inputEmail" class="col-lg-2 control-label">Name</label>
                                <div class="col-lg-10">
                                    <input type="text" class="form-control input-sm" id="inputName" placeholder="Name">
                                    <div class="msg"></div>
                                    {{!-- <span class="glyphicon glyphicon-exclamation-sign  form-control-feedback eedbackES hidden"></span>
                                    <span class="help-block error hidden"></span>  --}}
                                </div>
                            </div>
                            <div class="form-group has-feedback">
                                <label for="inputEmail" class="col-lg-2 control-label">Email</label>
                                <div class="col-lg-10">
                                    <input type="text" class="form-control input-sm" id="inputEmail" placeholder="Email">
                                    <div class="msg"></div>
                                     {{!-- <span class="glyphicon glyphicon-exclamation-sign  form-control-feedback"></span>
                                    <span class="help-block">please input email</span>
                                    <span class="help-block">wrong email</span>
                                    <span class="help-block">email already available</span> --}}
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="inputPassword" class="col-lg-2 control-label">Password</label>
                                <div class="col-lg-10">
                                    <input type="password" class="form-control input-sm" id="inputPassword" placeholder="Password">
                                    <div class="msg"></div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="inputPassword" class="col-lg-2 control-label">Confirm Password</label>
                                <div class="col-lg-10">
                                    <input type="password" class="form-control input-sm" id="inputCPassword" placeholder="Confirm Password">
                                    <div class="msg"></div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-lg-10 col-lg-offset-2">
                                    <button type="reset" class="btn btn-default cancel"> <a href="/">Cancel</a></button>
                                    <button type="submit" class="btn btn-primary signupBtn">Submit</button>
                                </div>
                            </div>
                        </div>
                    </fieldset>
            </div>
        </div>
    </div>
    <div class="col-lg-3">
    </div>
</div>
</div>
